Include ssh_usernames

Host *
  IdentitiesOnly yes

Host github.com
  IdentityFile ~/.ssh/id_github

Host vulcan*
  IdentityFile ~/.ssh/id_vulcan

Host eceprog*
  IdentityFile ~/.ssh/id_ece

Host ececomp*
  IdentityFile ~/.ssh/id_ece

Host ece337*
  IdentityFile ~/.ssh/id_ece337

Host comfibeats
  IdentityFile ~/.ssh/id_comfibeats

Host vulcanR
  Hostname vulcan.moe
  Port 4225

Host vulcanL
  Hostname 128.210.6.109

Host eceprogL
  Hostname eceprog.ecn.purdue.edu

Host eceprog1L
  Hostname eceprog1.ecn.purdue.edu

Host ececompL
  Hostname ececomp.ecn.purdue.edu

Host ececomp1L
  Hostname ececomp1.ecn.purdue.edu

Host eceprogR
  Hostname eceprog.ecn.purdue.edu
  ProxyJump vulcanR

Host eceprog1R
  Hostname eceprog1.ecn.purdue.edu
  ProxyJump vulcanR

Host ececompR
  Hostname ececomp.ecn.purdue.edu
  ProxyJump vulcanR

Host ececomp1R
  Hostname ececomp1.ecn.purdue.edu
  ProxyJump vulcanR

Host ece337L
  Hostname ececomp1.ecn.purdue.edu

Host ece337R
  Hostname ececomp1.ecn.purdue.edu
  ProxyJump vulcanR
  ForwardX11 yes

Host comfibeats
  Hostname 192.168.1.120
